  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.7.2",
    "@tauri-apps/cli": "^1.5.8",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.16",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "prettier": "^3.2.5",
    "typescript": "^5.0.2",
    "vite": "^5.0.0",
    "vue-tsc": "^1.8.5"
